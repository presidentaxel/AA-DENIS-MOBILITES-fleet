        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(rate(sqlalchemy_pool_checkout_seconds_sum[5m])) by (pool)",
          "legendFormat": "{{pool}} checkout sum",
          "refId": "A"
        }
      ],
      "title": "DB checkout time (sum rate)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
